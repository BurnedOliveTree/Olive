add(first: Int, second: Int): Int {
    return first + second;
}

subtract(first: Int, second: Int, third: Int): Int {
    return first - second - third;
}

multiply(first: Int, second: Int): Int {
    return first * second;
}

chainCall(number: Int): Int {
    return number.add(2).multiply(7).subtract(5, 6).add(5).multiply(2);
}

main(): Unit {
    var number: Int = 6;
    chainCall(number);
}